PRINT_H_D	PROC	NEAR
	;BX-ADRESS OF NUMBER
	PUSH	AX
	PUSH	CX
	PUSH	DX
	PUSH	SI
	MOV	SI,10
	XOR	CX,CX
	MOV	AX,[BX]
P_N_D1:
	XOR	DX,DX
	DIV	SI
	PUSH	DX
	INC	CX
	OR	AX,AX
	JZ	P_N_D2
	JMP	short	P_N_D1
P_N_D2:
	POP	DX
	ADD	DL,30H
	MOV	AH,2
	INT	21H
	LOOP	P_N_D2
	POP	SI
	POP	DX
	POP	CX
	POP	AX
	RET
ENDP
