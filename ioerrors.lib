FILE_ERRORS	PROC
	PUSH	CS
	POP	DS
	MOV	DX,SI
	LEA	SI,WRONG_NUMBER_OF_FUNCTION_
	DEC	AX
	JZ	PRINT_FILE_ERROR
	DEC	AX
	JZ	PRINT_FILE_NOT_FOUND
	DEC	AX
	JZ	PRINT_PATH_NOT_FOUND
	LEA	SI,NO_MORE_DESCRIPTORS_
	DEC	AX
	JZ	PRINT_FILE_ERROR
	LEA	SI,ACCESS_DENIED_
	DEC	AX
	JZ	PRINT_FILE_ERROR
	LEA	SI,WRONG_DESCRIPTOR_
	DEC	AX
	JZ	PRINT_FILE_ERROR
	LEA	SI,FILE_ERROR_
PRINT_FILE_ERROR:
	CALL	PRINT_WORD_OF_ERROR
	JMP	WRITE_WORD
PRINT_FILE_NOT_FOUND:
	LEA	SI,FILE_
	CALL	WRITE_WORD
	MOV	SI,DX
	CALL	WRITE_WORD
PRINT_FILE_ERROR_1:
	LEA	SI,NOT_FOUND_
	JMP	WRITE_WORD
PRINT_PATH_NOT_FOUND:
	LEA	SI,PATH_
	CALL	WRITE_WORD
	MOV	SI,DX
	CALL	WRITE_WORD
	JMP	SHORT	PRINT_FILE_ERROR_1
ENDP